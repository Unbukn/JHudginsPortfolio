    <div class="column is-6">
        <a href="#">
          <div class="box">
            <article class="media">
              <div class="media-left">
                <figure class="image is-64x64">
                  <img src="{{dataValues.picture_URL}}" alt="{{dataValues.picture_URL}}">
                </figure>
              </div>
              <div class="media-content">
                <div class="content">
                  <p>
                    <strong>{{dataValues.title}}</strong> <small>{{dataValues.type}}</small> <small>{{dataValues.init_date}}</small>
                    <br>
                    {{dataValues.desc_short}}
                  </p>
                <div class="content">
                  <p>
                    <hr>
                    {{dataValues.desc_long}}
                  </p>                  
                </div>
                <nav class="level is-mobile">
                  <div class="level-left">
                    <a class="level-item" aria-label="reply">
                      <span class="icon is-small">
                        <i class="fas fa-comment fa-lg"></i>
                      </span>
                    </a>
                    <a href="{{dataValues.live_link}}" class="level-item" aria-label="retweet">
                      <span class="icon is-small">
                        <i class="fas fa-link fa-lg"></i>
                      </span>
                    </a>
                    <a href="{{dataValues.repo_link}}" class="level-item" aria-label="like">
                      <span class="icon is-small">
                        <i class="fab fa-github-square fa-lg"></i>
                      </span>
                    </a>
                  </div>
                </nav>
              </div>
            </article>
          </div>
        </a>
      </div>